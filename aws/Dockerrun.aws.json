{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "sw-nginx",
      "image": "{$AWS_ACCOUNT_ID}.dkr.ecr.{$AWS_DEFAULT_REGION}.amazonaws.com/sw-nginx",
      "hostname": "sw-nginx",
      "essential": true,
      "memory": 300,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["sw-api", "sw-web"]
    },
    {
      "name": "sw-api",
      "image": "{$AWS_ACCOUNT_ID}.dkr.ecr.{$AWS_DEFAULT_REGION}.amazonaws.com/sw-api",
      "hostname": "sw-api",
      "essential": true,
      "memory": 200
    },
    {
      "name": "sw-web",
      "image": "{$AWS_ACCOUNT_ID}.dkr.ecr.{$AWS_DEFAULT_REGION}.amazonaws.com/sw-nginx",
      "hostname": "sw-web",
      "essential": true,
      "memory": 200
    }
  ]
}
