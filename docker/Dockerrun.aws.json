{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "nginx",
      "image": "tuedockerid1107/nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["web", "api"],
      "memory": 100
    },
    {
      "name": "web",
      "image": "tuedockerid1107/web",
      "hostname": "web",
      "essential": false,
      "memory": 300
    },
    {
      "name": "api",
      "image": "tuedockerid1107/api",
      "hostname": "api",
      "essential": false,
      "memory": 300
    }
  ]
}
